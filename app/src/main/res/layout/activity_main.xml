<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <FrameLayout
        android:id="@+id/FrameSala"
        android:layout_width="410dp"
        android:layout_height="50dp"
        android:background="#bffff7"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="35dp"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/ViewMesa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textFontWeight="100"
            android:text="Mesa: "

            />
    </FrameLayout>


    <FrameLayout
        android:id="@+id/FrameFactura"
        android:layout_width="412dp"
        android:layout_height="470dp"
        android:background="#bffff7"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="130dp"
        tools:ignore="MissingConstraints">

        <TableLayout
            android:id="@+id/TLFactura"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </TableLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/FrameImpresora"
        android:layout_width="410dp"
        android:layout_height="50dp"
        android:background="#bffff7"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="637dp"
        tools:ignore="MissingConstraints">

        <!--
        <ImageButton
            android:id="@+id/BotonImprimir"
            android:layout_width="66dp"
            android:layout_height="52dp"
            android:scaleType="centerInside"
            app:srcCompat="@mipmap/impresora"
            tools:ignore="MissingConstraints" /> -->
    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>